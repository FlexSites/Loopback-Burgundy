{
  "name": "Subscriber",
  "plural": "subscribers",
  "base": "OmniModel",
  "idInjection": true,
  "properties": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "required": true
    },
    "zip": {
      "type": "string"
    },
    "site": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "string",
      "default": "S"
    }
  },
  "validations": [],
  "relations": {
        "site": {
          "type": "belongsTo",
          "model": "Site",
          "foreignKey": "siteId"
        }
    },
  "acls": [
    {
      "model": "subscriber",
      "property": "*",
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "model": "order",
      "property": "*",
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "model": "*",
      "property": "*",
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": []
}
